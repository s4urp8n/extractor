{
  "name": "zver/extractor",
  "description": "Helps you extract exe, msi, 7z, zip, rar, cab. Also contain Encoder class",
  "minimum-stability": "dev",
  "config": {
    "optimize-autoloader": true
  },
  "autoload": {
    "psr-4": {
      "": "src/"
    },
    "files": [
      "src/Functions.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "": "tests/"
    }
  },
  "require": {
    "zver/common": "dev-master",
    "zver/string-helper": "dev-master",
    "zver/directory-walker": "dev-master",
    "zver/array-helper": "dev-master"
  },
  "require-dev": {
    "phpunit/phpunit": "*",
    "zver/meter": "*"
  }
}
